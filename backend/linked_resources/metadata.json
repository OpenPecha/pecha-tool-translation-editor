    "sherab_nyinpo": {
        "root": {
            "title": {
                "bo": "ཤེས་རབ་ཀྱི་ཕ་རོལ་ཏུ་ཕྱིན་པའི་སྙིང་པོ།",
                "en": "The Heart of the Perfection of Wisdom"
            },
            "author": {
                "en": "phakdoe"
            }
        },
        "commentary_1": {
            "title": {
                "bo": "བཻ་རོ་ཙ་ན། ཤེར་སྙིང་འགྲེལ་པ་སྔགས་སུ་བཀྲལ་པ།",
                "en": "Vairocana, Unravelling the Meaning of the Heart of Sutra According to the Mantra"
            },
            "author": {
                "en": "author of commentary_1"
            }
        },
        "commentary_2": {
            "title": {
                "bo": "ཕྱག་ན་རྡོ་རྗེ། ཤེར་ཕྱིན་སྙིང་པོའི་འགྲེལ་པ།",
                "en": "Chakna Dorje, Explaining the Heart Sutra"
            },
            "author": {
                "en": "author of commentary_2"
            }
        },
        "commentary_3": {
            "title": {
                "bo": "ཏཱ་ར་ནཱ་ཐ། ཤེས་རབ་སྙིང་པོའི་ཚིག་འགྲེལ།",
                "en": "Taranatha, Word for Word Commentary on the Heart Sutra"
            },
            "author": {
                "en": "author of commentary_3"
            }
        },
